<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proje">
    <uses-permission android:name="android.permission.CAMERA"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".com.example.proje.ogrenci_veli_activity.OgrenciActivity"
            android:label="@string/title_activity_ogrenci"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".com.example.proje.ogrenci_veli_activity.VeliActivity"
            android:label="@string/title_activity_veli"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".com.example.proje.ogretmenactivity.OgretmenActivity"
            android:label="@string/title_activity_ogretmen"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustResize"
            />
        <activity android:name=".com.example.proje.kayit_ve_login_activitiyleri.SistemKayit" />


        <activity android:name=".com.example.proje.kayit_ve_login_activitiyleri.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>